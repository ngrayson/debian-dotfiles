	},
	"display": {
		"separator": " > ",
		"color": {
			"keys": "green",
			"title": "cyan"
		},
		"key": {
			"width": 5,
			"type": "string"
		    // "type": "icon",
			// "type": "both"
		},
		"bar": {
			"width": 4,
			"border":{
				"right": "]",
				"left": "["
			}
		},
		"disableLinewrap": true,
		"hideCursor": true,
		"showErrors": true
	},
	"modules": [
		"break",
		// "title",
		// "separator",
		{ "type": "os", "key": "SYS", "format": "{10}" },
		{ "type": "host", "key": "HST", "format": "MNT Pocket" },
		// "kernel",

		{ "type": "uptime", "key": "UPT", "format": "{days}d {hours}h {minutes}m" },
		// "packages",
		{ "type": "shell", "key": "SHL" },
		{ "type": "terminal", "key": "TRM", "format": "{1}" },
		// "display",
		// "de",
		// "wm",
		// "wmtheme",
		// "theme",
		// "icons",
		// "font",
		// "cursor",
		// "terminalfont",
		// { "type": "cpu", "key": "CPU", "format": "{1} {2}" },
		// { "type": "gpu", "format": "{1} {2}" },
		{
			"type": "memory",
			"key": "MEM",
			"percent": {
				"type": ["num", "bar"]
			},

			"format": "{percentage-bar} {total}"
			// "format": "{percentage-bar} [{percentage}] {total}"
		},
		// {
		// 	"type": "disk",
		// 	"key": "DSK",
		// 	"format": "{size-used}/{size-total}"
		// },
		{
			"key": "DSK",
			"type": "disk",
			"format": "{size-percentage-bar} {size-total}",
			// "format": "{size-percentage-bar} [{size-percentage}] {size-total}",
			"percent": {
				"type": ["num", "bar"]
			}
		},
		// "swap",
		// "disk",
		// "localip",
