{
	"$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
	"logo": {
		"type": "iterm",
		// "type": "none",
		"source": "~/.config/wizterm-2.gif",
		// "width": 17,
		"height": 9,
		"preserveAspectRatio": true,
		"padding": {
			"top": 1,
			"right": 1,
			"left": 1
		},
		// "type": "small",
		// "printRemaining": true
		// "type": "chafa",
		// "chafa": {
